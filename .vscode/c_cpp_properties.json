{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/toolchain/i686-elf/lib/gcc/i686-elf/11.2.0/include",
                "${workspaceFolder}/src/bootloader",
                "${workspaceFolder}/build/sysroot/usr/include",
                "${workspaceFolder}/src/libs"
            ],
            "compilerPath": "${workspaceFolder}/toolchain/i686-elf/bin/i686-elf-gcc",
            "compilerArgs": [
                "--sysroot=${workspaceFolder}/build/sysroot",
                "-isystem=/usr/include",
                "-ffreestanding",
                "-MD", 
                "-Wall", 
                "-Wextra", 
                "-Wno-sign-compare",
                "-lk",
                "-lgcc",
                "-Isrc/kernel",
                "-Isrc/libs"
            ],
            "defines": [
                "__aurora__",
                "__i386__",
                "__I386__",
                "__x86__",
                "__is_libk"
            ],
            "cStandard": "c99",
            "cppStandard": "c++98",
            "intelliSenseMode": "gcc-x86"
        }
    ],
    "version": 4
}